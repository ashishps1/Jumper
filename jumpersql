 CREATE TABLE `jumperdr` (
`intJumperID` INT( 11 ) NOT NULL AUTO_INCREMENT PRIMARY KEY ,
`vcStationId` VARCHAR( 7 ) NOT NULL ,
`vcJumperNo` VARCHAR( 11 ) NULL ,
`vcTempJumperNo` VARCHAR( 11 ) NULL ,
`chUnit` VARCHAR( 1 ) NOT NULL ,
`vcLocation` VARCHAR( 100 ) NOT NULL ,
`vcDrawing` VARCHAR( 40 ) NULL ,
`vcReason` VARCHAR( 100 ) NULL ,
`vcRemarks` VARCHAR( 2000 ) NOT NULL ,
`vcProposedby` VARCHAR( 10 ) NOT NULL ,
`vcProposedbyDesig` VARCHAR( 10 ) NOT NULL ,
`vcProposedbySection` VARCHAR( 10 ) NOT NULL ,
`vcProposedbyDate` DATETIME NOT NULL ,
`vcApprovedby` VARCHAR( 10 ) NULL ,
`vcApprovedbyDesig` VARCHAR( 10 ) NULL ,
`vcApprovedbySection` VARCHAR( 10 ) NULL ,
`vcApprovedbyDate` DATETIME NULL ,
`vcApprovedTill` DATE NULL ,
`vcCSSD` VARCHAR( 10 ) NULL ,
`vcCSSDDesig` VARCHAR( 10 ) NULL ,
`vcCSSDSection` VARCHAR( 10 ) NULL ,
`vcCSSDDate` DATETIME NULL ,
`chCS` VARCHAR( 10 ) NULL ,
`chSD` VARCHAR( 10 ) NULL ,
`vcIssedby` VARCHAR( 10 ) NULL ,
`vcIssedbyDesig` VARCHAR( 10 ) NULL ,
`vcIssedbySection` VARCHAR( 10 ) NULL ,
`vcIssedbyDate` DATETIME NULL ,
`vcImplementedby` VARCHAR( 10 ) NULL ,
`vcImplementedbyDesig` VARCHAR( 10 ) NULL ,
`vcImplementedbySection` VARCHAR( 10 ) NULL ,
`vcImplementedbyDate` DATETIME NULL ,
`vcTestingDoneby` VARCHAR( 10 ) NULL ,
`vcTestingDonebyDesig` VARCHAR( 10 ) NULL ,
`vcTestingDonebySection` VARCHAR( 10 ) NULL ,
`vcTestingDonebyDate` DATETIME NULL ,
`vcQAVerifiedby` VARCHAR( 10 ) NULL ,
`vcQAVerifiedbyDesig` VARCHAR( 10 ) NULL ,
`vcQAVerifiedbySection` VARCHAR( 10 ) NULL ,
`vcQAVerifiedbyDate` DATETIME NULL ,
`vcNorApprovedby` VARCHAR( 10 ) NULL ,
`vcNorApprovedbyDesig` VARCHAR( 10 ) NULL ,
`vcNorApprovedbySection` VARCHAR( 10 ) NULL ,
`vcNorApprovedbyDate` DATETIME NULL ,
`vcNormalizedby` VARCHAR( 10 ) NULL ,
`vcNormalizedbyDesig` VARCHAR( 10 ) NULL ,
`vcNormalizedbySection` VARCHAR( 10 ) NULL ,
`vcNormalizedbyDate` DATETIME NULL ,
`vcPostNormalizedby` VARCHAR( 10 ) NULL ,
`vcPostNormalizedbyDesig` VARCHAR( 10 ) NULL ,
`vcPostNormalizedbySection` VARCHAR( 10 ) NULL ,
`vcPostNormalizedbyDate` DATETIME NULL ,
`vcQAVerifiedby2` VARCHAR( 10 ) NULL ,
`vcQAVerifiedby2Desig` VARCHAR( 10 ) NULL ,
`vcQAVerifiedby2Section` VARCHAR( 10 ) NULL ,
`vcQAVerifiedby2Date` DATETIME NULL ,
`chCategory` VARCHAR( 2 ) NULL ,
`chJASS` VARCHAR( 1 ) NULL ,
`chReqFuncTest` VARCHAR( 1 ) NULL ,
`vcRecommendation` VARCHAR( 100 ) NULL ,
`vcDescription` VARCHAR( 200 ) NULL ,
`vcUnitStatus` VARCHAR( 15 ) NULL ,
`chSafety` VARCHAR( 1 ) NULL ,
`chCancel` VARCHAR( 1 ) NULL ,
`vcUniqueId` VARCHAR( 23 ) NULL ,
`vcExpiryDateAndTime` VARCHAR( 25 ) NULL ,
`vcSubsystem` VARCHAR( 3 ) NOT NULL ,
`vcSystem` VARCHAR( 5 ) NOT NULL ,
`vcSystemNo` VARCHAR( 3 ) NOT NULL ,
`vcEqpt` VARCHAR( 10 ) NOT NULL ,
`vcEqptNo` VARCHAR( 5 ) NOT NULL ,
`vcAdditionalCode` VARCHAR( 5 ) NULL ,
`vcComponent` VARCHAR( 7 ) NULL ,
`vcComponentNo` VARCHAR( 3 ) NULL ,
`vcDRNo` VARCHAR( 14 ) NOT NULL ,
`vcJRCDescription` VARCHAR( 50 ) NULL ,
`vcProposedTill` DATE NOT NULL ,
`vcStatus` VARCHAR( 10 ) NULL
) ENGINE = MYISAM 
